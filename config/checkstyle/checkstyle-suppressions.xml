<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="PackageName"/>
    <suppress message="Missing a Javadoc comment"
              files="./src/test/*"/>
    <suppress checks="^*Javadoc*|AtclauseOrder|NonEmptyAtclauseDescription|RequireEmptyLineBeforeBlockTagGroup|WriteTag"
              files="./src/test/*"/>
</suppressions>